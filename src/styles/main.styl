      width 90%
      overflow-y scroll
    div
      display flex
      justify-content space-between
      @media screen and (max-width: 400px)
        padding-bottom: 20px;
    #close
      cursor pointer
      background none
      color $secondary-color
    p
      color $text-color
      line-height 25px
    span
      display block
      margin-top 40px
      background $main-color
      color #fff
      width 100px
      text-align center
      padding 10px 20px
      border-radius 20px
      cursor pointer
