@require "reset"

$main-color = #FF9800
$secondary-color = #4A508A
$text-color = rgba(0,0,0, .7)

@keyframes down {
  0% {
    position absolute
    bottom 0
  }

  50% {
    position absolute
    bottom 25px
  }

  100% {
    position absolute
    bottom 0
  }

}


setBackground(link)
  background url(link) no-repeat center
  background-size cover

fs(val)
  font-size val

.flying-block
  width 70%
  background #fff
  height 550px
  position absolute
  top -275px
  left 50%
  margin-left -35%
  box-shadow: 0px -5px 74px -13px rgba($secondary-color, .2)
  display flex;
  flex-direction: column;
  justify-content: space-around;
  @media screen and (max-width: 1270px)
    height 600px
    top -300px


global-reset()

html, body
  font-family 'Roboto', sans-serif

*
  margin 0
  padding 0

section#one
  height: 100vh
section
  &:not(#one)
    min-height 100vh

.toggle-nav
  width 30px
  height 30px
  position fixed
  right 25px
  top 25px
  cursor pointer
  z-index 999999
  &.active
    span
      background $secondary-color
      &:first-child
        transform translateY(8px) rotate(-45deg)
      &:last-child
        transform translateY(-8px) rotate(45deg)
      &:nth-child(2)
        transition all .3s ease
        transform translateX(-30px)
        opacity 0
  span
    width 100%
    height 1px
    margin-top 7px
    background $main-color
    transition transform .5s ease
    @media screen and (max-width: 1000px)
      display block

.n
  width 100%
  height 70px
  line-height 70px
  margin 0 auto
  position fixed
  top 0
  right -100%
  display none
  opacity 0
  z-index 9999999999
  color $secondary-color
  font-weight 300
  background #fff
  margin 0 auto
  text-align center
  transition all 1s ease
  box-shadow: 0px 4px 12px -5px rgba(0,0,0,0.5)
  &.active
    opacity 1
    display block
    right 0

.m-w
  z-index 9999999999999
  height 100%
  width 100%
  background rgba(0, 0, 0, 0.6);
  position fixed
  top 0
  left 0
  right 0
  bottom 0
  justify-content center
  align-items center
  display none
  opacity 0
  transition opacity 2s ease
  &.active
    display flex
    opacity 1
    .w
      opacity 1
      @media screen and (max-width: 400px)
        height 100vh
      p
        &:first-child
          @media screen and (max-width: 400px)
            margin-top: 20px
  .w
    opacity 0
    min-height 35%
    width 50%
    background #fff
    box-shadow: 0px -5px 74px -13px rgba($secondary-color, .2)
    padding 20px
